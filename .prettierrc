{
  "trailingComma": "all",
  "tabWidth": 2,
  "printWidth": 120,
  "singleQuote": true,
  "proseWrap": "always",
  "htmlWhitespaceSensitivity": "ignore",
  "endOfLine": "auto",
  "useTabs": false,
  "plugins": ["prettier-plugin-svelte"],
  "pluginSearchDirs": ["."],
  "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
